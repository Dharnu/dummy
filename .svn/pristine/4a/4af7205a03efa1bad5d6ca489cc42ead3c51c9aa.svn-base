}

@media only screen and (max-width: 770px) {

    header {
        height: 11%;
        font-size: 35px;
        font-family: 'Bryant';
    }
    section {
        height:68%;
    }
    footer {
        height: 21%;
        display: flex;
    }
    .content-body {
        background-color: #ffffff;
        margin: auto;
        width: 97%;
        height: 100%;
    }
    .cal-container {
        width: 94%;
        height: 100%;
        margin: 0 auto;
    }
    .content-shadow{
        -webkit-box-shadow: 7px 7px 1px 0px #46B6AD;
        -moz-box-shadow: 7px 7px 1px 0px #46B6AD;
        box-shadow: 7px 7px 1px 0px #46B6AD;
    }
    .cal-shadow{
        -webkit-box-shadow: 7px 7px 1px 0px #13A2BA;
        -moz-box-shadow: 7px 7px 1px 0px #13A2BA;
        box-shadow: 7px 7px 1px 0px #13A2BA;
    }
    .cal-shadow1{
        -webkit-box-shadow: 7px 40px 1px 0px #13A2BA;
        -moz-box-shadow: 7px 40px 1px 0px #13A2BA;
        box-shadow: 7px 40px 1px 0px #13A2BA;
    }
    .whole-shadow {
        box-shadow: -1px 1px 5px 8px rgba(93, 218, 218, 0.27);
        -webkit-box-shadow: -1px 1px 5px 8px rgba(93, 218, 218, 0.27);
        -moz-box-shadow: -1px 1px 5px 8px rgba(93, 218, 218, 0.27);
    }

    /*time selector*/

    .select-event-time-main {
        width: 50%;
        height: 55%;
        background-color: #16bad6;
    }
    .select-event-time {
        width: 100%;
        height: 100%;
    }
    .time-actions {
        width: 100%;
        height: 15%;
    }
    .time-actions > div {
        width: 30%;
        height: 70%;
        background-color: #fcb040;
        color: #ffffff;
        font-family: 'Myriad-Bold';
        font-size: 20px;
        border-radius: 8px;
    }
    .select-time {
        height: 77%;
        margin: auto;
        width: 100%;
    }
    .select-time > div {
        width: 30%;
    }
    .select-hours{
        overflow: scroll;
        font-size: 20px;
        font-family: 'Myriad-Bold';
        border-right: 1px solid;
    }
    .select-minutes {
        overflow: scroll;
        font-size: 20px;
        font-family: 'Myriad-Bold';
        border-right: 1px solid;
    }
    .select-mode {
        font-size: 20px;
        font-family: 'Myriad-Bold';
    }
    .select-hours > div {
        cursor: pointer;
    }
    .select-minutes > div {
        cursor: pointer;
    }
    .select-mode > div {
        cursor: pointer;
    }




    /*Multi Calendar*/
    .grid-box-scroll {

    }
    .grid-box-main {
        width: 95%;
        flex-wrap: wrap;
        height: 95%;
        margin: auto;
        overflow: scroll;
    }
    .cal_box {
        width: 31%;
        height: 118px;
        font-size: 30px;
        font-family: 'Myriad-Bold';
    }
    .cal_box > span {
        padding-top: 3%;
    }
    .grid-box-main >div:nth-child(odd) {
        background-color: #fcb040;
    }
    .grid-box-main >div:nth-child(even) {
        background-color: #59c4bc;
    }
    .grid-box-main > div {
        margin: 1%;
    }
    /*Multi calendar ends*/

    /*Pop up starts*/
    /*Alert Popup starts*/
    .alert-popup {
        background: #fff;
        width: 85%;
        height: 25%;
        font-family: 'Myriad-Regular';
        font-size: 25px;
        color: #969090;
        border-radius: 10px;
    }
    .overlay {
        display: none;
        position: absolute;
        top: 0;
        bottom: 0;
        left: 0;
        right: 0;
        background: rgba(0, 0, 0, 0.55);
        transition: opacity 500ms;
    }
    .alert-icon-main {
        width: 100%;
        height:28%;
        background-color: #F4B936;
        border-top-left-radius: 10px;
        border-top-right-radius: 10px;
    }
    .alert-icon {
        width: 6.5%;
        height: 65%;
        color: transparent;
        border: 6px solid #ffffff;
        border-radius: 50px;
        margin: auto;
    }
    .exc-symbol {
        margin-top: 25%;
        color: #ffffff;
        font-size: 45px;
    }
    .alert-message {
        width: 100%;
        height: 40%;
        text-align: center;
        font-size: 29px;
    }
    .alert-action {
        width: 100%;
        height: 27%;
    }
    .alert-action > div {
        width: 20%;
        height: 90%;
        background-color: #F4B936;
        color: #ffffff;
        border-radius: 10px;
    }
    /*Alert popup ends*/
    
    /*Confirm popup starts*/
    .confirm-popup {
        width: 75%;
        height: 34%;
        background-color: #ffffff;
        border-radius: 15px;
    }
    .popup_shadow{
        box-shadow:  0 0 10px 10px #575757;
        -moz-box-shadow: 0 0 10px  10px #575757;
        -webkit-box-shadow: 0 0 10px 10px #575757;
        -o-box-shadow: 0 0 10px 10px #575757;
    }
    .popup-header {
        width: 100%;
        height: 35%;
        background-color: #59C4BC;
        border-top-left-radius: 15px;
        border-top-right-radius: 15px;
    }
    .popup-msg {
        height: 35%;
        overflow: auto;
        font-size: 37px;
        text-align: center;
        color: #59C4BC;
        font-family: 'Myriad-Bold'
    }
    .popup-action-main {
        width: 100%;
        height: 30%;
    }
    .popup-action {
        width: 24%;
        height: 52%;
        background-color: #F4A639;
        color: #ffffff;
        margin-left: 2%;
        border-radius: 8px;
        font-family: 'Myriad-Bold';
        font-size: 25px;
    }
    .confirm-alert_icon {
        width: 12%;
        height: 54%;
        color: transparent;
        border: 6px solid #ffffff;
        border-radius: 50px;
        margin: auto;
    }
    /*Confirm popup ends*/
    /*Pop up ends*/
}